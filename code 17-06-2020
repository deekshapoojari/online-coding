#include<stdio.h>
void main()
{
    int a[15], val = 1, i, n;
    printf("Enter the value of N: ");
    scanf("%d", &n);
    printf("Enter the numbers:\n");
    for(i = 0; i < n; i++)
        scanf("%d", &a[i]);
    for (int i = 0; i < n && a[i] <= val; i++)
       val = val + a[i];
    printf("\n%d\n", val);
}
